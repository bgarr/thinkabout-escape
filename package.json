  "scripts": {
    "build": "bin/vite build",
    "lint:js": "eslint --quiet",
    "lint:ruby": "bundle exec rubocop --parallel -c ./.rubocop.yml",
    "test": "",
    "cypress:compile": "RAILS_ENV=cypress bundle install",
    "cypress:rails": "RAILS_ENV=cypress bundle exec rails db:prepare || bundle exec rails s -e cypress -p 5002 -d",
    "cypress:local": "yarn cypress:compile && yarn cypress:rails",
    "cypress:headless": "export NO_PROXY=localhost && yarn cypress run --e2e --config-file \"./cypress.config.ts\"",
    "cypress:ci": "yarn cypress:local && yarn cypress:headless"
  }
